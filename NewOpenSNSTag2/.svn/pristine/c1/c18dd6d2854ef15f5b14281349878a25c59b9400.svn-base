@charset "utf-8";
.clearfix {
  zoom: 1;
}
.clearfix:after {
  content: '';
  display: block;
  clear: both;
}
.hide {
  display: none;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.font_arial {
  font-family: 'Arial';
}
.hsBody {
  max-width: 800px;
}
input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
  color: #aaaaaa;
}
input:-moz-placeholder,
textarea:-moz-placeholder {
  color: #aaaaaa;
}
input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
  color: #aaaaaa;
}
.weui-picker__item {
  padding: 7px 0 7px;
  height: 34px;
  font-size: 16px;
}
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  -webkit-tap-highlight-color: transparent;
  /* For some Androids */
}
body {
  font-family: "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 14px;
  line-height: 1.428571429;
  color: #333333;
  background-color: #ffffff;
}
li {
  list-style: none;
}
img {
  border: none;
}
input,
select,
textarea {
  outline: none;
  border: none;
  background: none;
  -webkit-appearance: none;
}
textarea {
  resize: none;
}
a {
  outline: none;
  text-decoration: none;
  -webkit-tap-highlight-color: transparent;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-size: 12px;
  font-weight: normal;
}
i {
  font-style: normal;
}
/*个人中心*/
.spaceLine {
  height: 0.4rem;
  background: #f8f5f6;
}
.centerTop {
  height: 4.48rem;
  position: relative;
  padding-left: 0.8rem;
  padding-top: 0.92rem;
}
.centerTop .title {
  font-size: 1.2rem;
  font-weight: bold;
  color: #333333;
  padding-right: 0.9rem;
  line-height: 1.7rem;
  display: inline-block;
  max-width: 10.6rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.centerTop .con {
  height: 1.16rem;
  line-height: 1.16rem;
  font-size: 0.6rem;
  color: #999999;
}
.centerTop .head {
  width: 2.4rem;
  height: 2.4rem;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  position: absolute;
  right: 0.8rem;
  top: 50%;
  margin-top: -1.2rem;
}
.centerTop .head img {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}
.centerCon {
  padding: 0px 0.8rem;
}
.centerCon li {
  line-height: 0.8rem;
  padding: 0.6rem 0px;
  font-size: 0.68rem;
  color: #333333;
  border-bottom: 1px solid #e9e9e9;
  position: relative;
}
.centerCon li span.title {
  float: left;
  color: #333333;
}
.centerCon li .con {
  padding-left: 3.6rem;
  text-align: right;
  color: #999999;
}
/*个人中心-编辑资料*/
.editInfoCon {
  padding: 0.98rem 0.8rem;
}
.editInfoCon li {
  height: 1.3rem;
  line-height: 1.3rem;
  margin-bottom: 0.8rem;
}
.editInfoCon li span {
  display: inline-block;
  width: 2.7rem;
  font-size: 0.72rem;
  color: #333333;
  float: left;
}
.editInfoCon li input {
  width: 10.4rem;
  height: 100%;
  border: 1px solid #cccccc;
  font-size: 0.6rem;
  color: #333333;
  padding: 0px 0.4rem;
}
.editInfoCon li textarea {
  height: 2.2rem;
  width: 10.4rem;
  border: 1px solid #cccccc;
  padding: 0.12rem 0.4rem;
  font-size: 0.6rem;
  color: #333333;
}
.centerBtnWarp {
  position: fixed;
  bottom: 0px;
  height: 3.16rem;
  width: 100%;
  background: #fff;
  z-index: 10;
  padding-top: 0.4rem;
}
.centerBtnWarp .centerBtn {
  width: 12rem;
  height: 1.7rem;
  line-height: 1.7rem;
  text-align: center;
  background: #ffea10;
  -webkit-border-radius: 0.2rem;
  -moz-border-radius: 0.2rem;
  border-radius: 0.2rem;
  display: block;
  margin: 0 auto;
  font-size: 0.68rem;
  color: #333333;
}
.twoBtn {
  padding: 0px 1.1rem;
  padding-top: 0.8rem;
}
.twoBtn a.centerBtn {
  width: 6rem;
}
.twoBtn a:nth-child(2) {
  background: #494172;
  color: #ffffff;
}
/*开具发票*/
.receiptWarp {
  padding: 0px 0.8rem;
}
.receiptWarp li {
  height: 2.7rem;
  line-height: 2.7rem;
  font-size: 0.72rem;
  color: #333333;
  border-bottom: 1px solid #e9e9e9;
  overflow: hidden;
}
.receiptWarp .moreIcon {
  width: 1.2rem;
  height: 100%;
  display: inline-block;
  float: right;
  background: url('../i/moreIcon.png') no-repeat right center;
  background-size: 0.32rem auto;
}
/*按订单开具发票*/
.receiptOrderWarp {
  background: #fff;
}
.receiptOrderWarp li {
  border-bottom: 1px solid #eeeeee;
  position: relative;
  height: 4rem;
  padding: 0.6rem 0.4rem;
  padding-left: 1.8rem;
}
.receiptOrderWarp li i {
  display: block;
  width: 1.02rem;
  height: 1.02rem;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  margin-top: -0.5rem;
  border: 1px solid #a8a8a8;
  left: 0.4rem;
}
.receiptOrderWarp li i.selected {
  background: #ffea10 url('../i/okIcon.png') no-repeat center center;
  background-size: 0.56rem auto;
  border: 1px solid #ffea10;
}
.receiptOrderWarp li .imgOuter {
  width: 2.8rem;
  height: 2.8rem;
  float: left;
}
.receiptOrderWarp li .imgOuter img {
  width: 100%;
  height: 100%;
}
.receiptOrderWarp li .con {
  padding-left: 3.28rem;
  padding-right: 2.7rem;
}
.receiptOrderWarp li .con h3 {
  height: 1.18rem;
  line-height: 1.18rem;
  font-size: 0.72rem;
  color: #333333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.receiptOrderWarp li .con p {
  font-size: 0.6rem;
  color: #999999;
  height: 0.92rem;
  line-height: 0.92rem;
  margin-top: 0.7rem;
}
.receiptOrderWarp li .con .money {
  height: 2.8rem;
  line-height: 2.8rem;
  font-size: 0.68rem;
  color: #ff0000;
  display: inline-block;
  position: absolute;
  right: 0.6rem;
  top: 50%;
  margin-top: -1.4rem;
}
.receiptOrderWarp li:last-child {
  border: none;
  border: 0px;
}
.receiptOrderBottom {
  height: 2.6rem;
  line-height: 2.6rem;
  width: 100%;
  position: fixed;
  bottom: 0px;
  z-index: 1000;
  background: #ffffff;
  padding-left: 4.3rem;
  padding-right: 5rem;
  font-size: 0.6rem;
  color: #333333;
}
.receiptOrderBottom .btn {
  display: block;
  width: 4.4rem;
  height: 1.5rem;
  line-height: 1.5rem;
  text-align: center;
  background: #494172;
  color: #ffffff;
  font-size: 0.6rem;
  -webkit-border-radius: 0.16rem;
  -moz-border-radius: 0.16rem;
  border-radius: 0.16rem;
  position: absolute;
  right: 0.6rem;
  top: 0.56rem;
}
.receiptOrderBottom .checkAllWarp {
  height: 2.6rem;
  position: absolute;
  left: 0.6rem;
  top: 0px;
  font-size: 0.68rem;
  color: #333333;
}
.receiptOrderBottom .checkAllWarp i {
  display: block;
  width: 1.02rem;
  height: 1.02rem;
  border: 1px solid #a9a9a9;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  float: left;
  margin-right: 0.4rem;
  margin-top: 0.7rem;
}
.receiptOrderBottom .checkAllWarp i.selected {
  background: #ffea10 url('../i/okIcon.png') no-repeat center center;
  background-size: 0.56rem auto;
  border: 1px solid #ffea10;
}
.cor_red {
  color: #ff0000 !important;
}
/*开票信息*/
.receiptInfoWarp {
  background: #fff;
  padding: 0px 0.6rem;
}
.receiptInfoWarp .title {
  height: 2.88rem;
  line-height: 2.88rem;
  font-size: 0.96rem;
  color: #333333;
  font-weight: bold;
}
.receiptInfoWarp .receiptType .typeTitle {
  height: 1.16rem;
  line-height: 1.16rem;
  font-size: 0.72rem;
  color: #333333;
}
.receiptInfoWarp .receiptType .typeCon {
  overflow: hidden;
  padding: 0.4rem 0px;
  padding-left: 1rem;
}
.receiptInfoWarp .receiptType .typeCon i {
  display: inline-block;
  width: 1.02rem;
  height: 1.02rem;
  border: 1px solid #a9a9a9;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  vertical-align: middle;
  margin-bottom: 0.12rem;
  margin-right: 0.4rem;
}
.receiptInfoWarp .receiptType .typeCon > div {
  font-size: 0.6rem;
  color: #333333;
  float: left;
  margin-right: 1.6rem;
}
.receiptInfoWarp .receiptType .typeCon > div.selected i {
  background: #ffea10 url('../i/okIcon.png') no-repeat center center;
  background-size: 0.56rem auto;
  border: 1px solid #ffea10;
}
.receiptInfoWarp .con {
  padding-bottom: 0.4rem;
}
.receiptInfoWarp li {
  margin-bottom: 0.6rem;
  height: 3.2rem;
  line-height: 1.3rem;
  font-size: 0.6rem;
  color: #333333;
}
.receiptInfoWarp li .subTitle {
  width: 3.64rem;
  display: inline-block;
  font-size: 0.72rem;
  color: #333333;
  float: left;
}
.receiptInfoWarp li input {
  width: 9.6rem;
  height: 1.3rem;
  border: 1px solid #cccccc;
  padding: 0px 0.3rem;
  font-size: 0.6rem;
  color: #333333;
}
.receiptInfoWarp li .subCon {
  padding-left: 3.64rem;
  position: relative;
}
.receiptInfoWarp li .arrowInputWarp {
  position: relative;
  width: 9.6rem;
}
.receiptInfoWarp li .arrowInputWarp .arrowIcon {
  display: block;
  width: 1.3rem;
  height: 1.3rem;
  background: url('../i/arrowIcon.png') no-repeat center center;
  background-size: 0.56rem auto;
  position: absolute;
  top: 0px;
  right: 0px;
  transition: all ease 0.3s;
  transition: all .3s ease;
}
.receiptInfoWarp li .selectPopWarp {
  width: 9.6rem;
  background: #fff;
  position: absolute;
  top: 1.3rem;
  left: 3.64rem;
  box-shadow: 0px 0.04rem 0.1rem rgba(0, 0, 0, 0.1);
  padding-top: 0.32rem;
  padding-bottom: 0.52rem;
  display: none;
  z-index: 100;
}
.receiptInfoWarp li .selectPopWarp li {
  height: 1.52rem;
  line-height: 1.52rem;
  font-size: 0.6rem;
  color: #333333;
  margin-bottom: 0px;
  padding: 0px 0.3rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.receiptInfoWarp li .open .selectPopWarp {
  display: block;
}
.receiptInfoWarp li .open input {
  border: 1px solid #ffea10;
  background: #ffea10;
}
.receiptInfoWarp li .open .arrowIcon {
  transform: rotate(180deg);
}
.receiptInfoWarp li .cell:after {
  content: '(单位:平米)';
  font-size: 0.6rem;
  color: #333333;
  position: absolute;
  right: -3.4rem;
  top: 0px;
}
.receiptInfoWarp li .troubleWarp textarea {
  width: 9.6rem;
  height: 5.5rem;
  border: 1px solid #cccccc;
  padding: 0.4rem;
  font-size: 0.6rem;
  color: #333;
  padding-bottom: 1.2rem;
}
.receiptInfoWarp li .troubleWarp span {
  font-size: 0.56rem;
  color: #999999;
  position: absolute;
  right: 0.8rem;
  bottom: 0.4rem;
}
.receiptInfoWarp .radioCon {
  overflow: hidden;
}
.receiptInfoWarp .radioCon i {
  display: inline-block;
  width: 1.02rem;
  height: 1.02rem;
  border: 1px solid #a9a9a9;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  vertical-align: middle;
  margin-bottom: 0.12rem;
  margin-right: 0.4rem;
}
.receiptInfoWarp .radioCon > div {
  font-size: 0.6rem;
  color: #333333;
  float: left;
  margin-right: 1.6rem;
}
.receiptInfoWarp .radioCon > div.selected i {
  background: #ffea10 url('../i/okIcon.png') no-repeat center center;
  background-size: 0.56rem auto;
  border: 1px solid #ffea10;
}
.reasonArea {
  width: 100%;
  height: 7.2rem;
  border: 1px solid #cccccc;
  font-size: 0.6rem;
  color: #333333;
  padding: 0.4rem;
  margin-top: 0.32rem;
}
/*开票信息-提交成功*/
.receiptSuccIcon {
  display: block;
  width: 3.52rem;
  height: 3.52rem;
  background: #09bb07 url('../i/okIcon_white.png') no-repeat center center;
  background-size: 1.88rem auto;
  margin: 0 auto;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  margin-top: 2rem;
}
.receiptSuccTip {
  height: 1.86rem;
  line-height: 1.86rem;
  text-align: center;
  font-size: 0.8rem;
  color: #000000;
}
.receiptSuccCon {
  width: 11.8rem;
  height: 6.2rem;
  background: #fff;
  margin: 0 auto;
  padding: 0px 0.6rem;
  position: relative;
  line-height: 1.1rem;
  font-size: 0.6rem;
  color: #666;
  text-align: center;
  border-left: 1px solid #f4f2ff;
  border-right: 1px solid #f4f2ff;
  padding-top: 1rem;
}
.receiptSuccCon:before {
  content: '';
  width: 12.8rem;
  height: 0.88rem;
  background: url('../i/SuccTip_top.png') no-repeat center center;
  background-size: 100% 100%;
  position: absolute;
  top: 0px;
  left: 50%;
  margin-left: -6.4rem;
}
.receiptSuccCon:after {
  content: '';
  width: 11.78rem;
  height: 1.18rem;
  background: url('../i/SuccTip_bottom.png') no-repeat center center;
  background-size: 100% 100%;
  position: absolute;
  bottom: -1rem;
  left: 0px;
}
.succDesc {
  line-height: 0.88rem;
  font-size: 0.72rem;
  color: #666666;
  text-align: center;
  padding-top: 0.4rem;
}
/*开票历史*/
.historyWarp {
  padding: 0px 0.8rem;
}
.historyWarp li {
  border-bottom: 1px solid #eeeeee;
  padding: 0.6rem 0px;
  height: 4rem;
  position: relative;
}
.historyWarp .imgWarp {
  width: 2.8rem;
  height: 2.8rem;
  float: left;
}
.historyWarp .imgWarp img {
  width: 100%;
  height: 100%;
}
.historyWarp .con {
  padding-left: 3.2rem;
}
.historyWarp .title {
  height: 1.16rem;
  line-height: 1.16rem;
  font-size: 0.72rem;
  color: #333333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  padding-right: 3rem;
}
.historyWarp .time {
  height: 0.96rem;
  line-height: 0.96rem;
  margin-top: 0.68rem;
  font-size: 0.6rem;
  color: #999999;
}
.historyWarp .status {
  display: inline-block;
  height: 4rem;
  line-height: 4rem;
  font-size: 0.6rem;
  color: #999999;
  font-weight: bold;
  right: 0px;
  top: 0px;
  position: absolute;
}
/*我的订单*/
.myOrderNav {
  height: 1.68rem;
  width: 100%;
  position: fixed;
  top: 0px;
  z-index: 1000;
  background: #fff;
}
.myOrderNav ul {
  display: flex;
}
.myOrderNav li {
  flex: 1;
  height: 2.68rem;
  line-height: 2.68rem;
  text-align: center;
}
.myOrderNav a {
  padding: 0.3rem;
  font-size: 0.72rem;
  color: #333333;
  border-bottom: 3px solid #ffffff;
}
.myOrderNav li.selected a {
  color: #494172;
  font-weight: bold;
  border-bottom: 3px solid #ffea10;
}
/*门店消杀*/
.questionWarp {
  padding: 0px 0.7rem;
}
.questionOne {
  margin-bottom: 0.6rem;
}
.questionTitle {
  font-size: 0.6rem;
  color: #313131;
  height: 1.36rem;
  line-height: 1.36rem;
  font-weight: bold;
}
.questionCon {
  font-size: 0.6rem;
  color: #888888;
  line-height: 1rem;
}
.questionCon .title {
  float: left;
}
.questionCon .con {
  padding-left: 1.24rem;
}
/*订单详情*/
.orderDetail {
  padding: 0px 0.6rem;
}
.orderDetail li {
  padding: 0.4rem 0px;
  line-height: 0.9rem;
}
.orderDetail .title {
  float: left;
  font-size: 0.72rem;
  color: #999999;
}
.orderDetail .con {
  padding-left: 3.6rem;
  font-size: 0.72rem;
  color: #333333;
  white-space:normal;
  word-break:break-all;
}
.orderDetail .cell {
  font-size: 0.6rem;
}
.orderDetail .phoneIcon {
  display: inline-block;
  width: 0.9rem;
  height: 0.9rem;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  background: #494172 url('../i/phoneIcon.png') no-repeat center center;
  background-size: 0.46rem auto;
  margin-left: 1rem;
  vertical-align: middle;
  margin-bottom: 0.12rem;
}
.orderDetailBottom {
  padding: 0px 0.6rem;
}
.orderDetailBottom .oneline {
  height: 1.28rem;
  line-height: 1.28rem;
  font-size: 0.72rem;
  color: #333333;
}
.orderDetailBottom .priceWarp {
  height: 1.5rem;
  line-height: 1.5rem;
  color: #333333;
  font-size: 0.96rem;
  font-weight: bold;
}
.orderDetailBottom .priceWarp_paidan {
  font-size: 0.72rem;
  color: #333;
  height: 1.5rem;
  line-height: 1.5rem;
}
.orderDetailBottom .priceWarp_paidan span {
  font-size: 0.56rem;
}
.orderDetailBottom .reasonWarp h4 {
  height: 1.36rem;
  line-height: 1.36rem;
  font-size: 0.72rem;
  color: #ff0000;
}
.orderDetailBottom .reasonWarp p {
  line-height: 0.66rem;
  font-size: 0.56rem;
  color: #333333;
}
/*申请售后*/
.serviceReason {
  padding: 0.4rem 0.8rem;
}
.serviceReason h3 {
  height: 1.92rem;
  line-height: 1.92rem;
  font-size: 0.72rem;
  color: #999999;
}
.serviceReason .con {
  width: 100%;
  height: 7.2rem;
  position: relative;
}
.serviceReason .con textarea {
  width: 100%;
  height: 7.2rem;
  border: 1px solid #cccccc;
  padding: 0.4rem;
  font-size: 0.72rem;
  color: #333333;
  padding-bottom: 1.2rem;
}
.serviceReason .con span {
  font-size: 0.56rem;
  color: #999999;
  position: absolute;
  right: 0.4rem;
  bottom: 0.4rem;
}
/*弹出框*/
.popLayer {
  width: 100%;
  position: fixed;
  top: 0px;
  bottom: 0px;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.5);
  /*电话沟通结果*/
}
.popLayer .popWarp {
  width: 10.8rem;
  background: #fff;
  min-height: 8.8rem;
  -webkit-border-radius: 0.2rem;
  -moz-border-radius: 0.2rem;
  border-radius: 0.2rem;
  padding: 0px 0.9rem;
  position: absolute;
  top: 40%;
  left: 50%;
  margin-left: -5.4rem;
  margin-top: -4.4rem;
}
.popLayer .popWarp h3 {
  height: 2.66rem;
  line-height: 2.66rem;
  font-size: 0.96rem;
  color: #333333;
  font-weight: bold;
}
.popLayer .popWarp p {
  line-height: 0.96rem;
  font-size: 0.68rem;
  color: #666666;
}
.popLayer .popWarp a.btn {
  display: block;
  height: 1.5rem;
  line-height: 1.5rem;
  width: 8rem;
  background: #ffea10;
  -webkit-border-radius: 0.16rem;
  -moz-border-radius: 0.16rem;
  border-radius: 0.16rem;
  text-align: center;
  margin: 0 auto;
  font-size: 0.6rem;
  color: #333333;
  margin-top: 0.86rem;
}
.popLayer .codeWarp {
  width: 10rem;
  min-height: 12.24rem;
  background: #ffea10 url('../i/codeTop.png') no-repeat top center;
  background-size: 100% auto;
  -webkit-border-radius: 0.2rem;
