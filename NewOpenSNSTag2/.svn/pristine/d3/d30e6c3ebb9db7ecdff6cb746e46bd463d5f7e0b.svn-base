        $sReqData = $GLOBALS['HTTP_RAW_POST_DATA'];

        $sMsg = ""; // è§£æä¹‹åçš„æ˜æ–‡
        $wxcpt = new \WXBizMsgCrypt ($this->token, $this->encodingAesKey, $this->corpId);
        $errCode = $wxcpt->DecryptMsg($sReqMsgSig, $sReqTimeStamp, $sReqNonce, $sReqData, $sMsg);

        if ($errCode == 0) {
